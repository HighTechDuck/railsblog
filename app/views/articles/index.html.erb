<p id="notice"><%= notice %></p>

<h1>News Articles BETA</h1>
    <% @articles.each do |article| %>
    <h2><%= article.title %></H2>
        <td><%= article.body %></td>
        <td><%= link_to 'View', article %></td>
        <td><%= link_to 'Edit', edit_article_path(article) %></td>
        <td><%= link_to 'Destroy', article, method: :delete, data: { confirm: 'Destroy Article' } %></td>
      </tr>
    <% end %>
